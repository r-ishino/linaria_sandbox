{
  "name": "linaria_sandbox",
  "version": "0.1.0",
  "main": "index.js",
  "repository": "git@github.com:r-ishino/linaria_sandbox.git",
  "author": "r-ishino <r.ishino0413@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "node esbuild.mjs",
    "build:type": "tsc -d -p tsconfig.build.json",
    "build:plugin": "esbuild --bundle --format=esm --platform=node --outfile=custom-wyw.js custom-esbuild-wyw.ts",
    "build:samplew": "esbuild --bundle --format=esm --platform=node --outfile=custom-sample.js custom-esbuild-sample.ts --watch"
  },
  "type": "module",
  "files": [
    "lib"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./lib/index.d.ts",
      "module": "./lib/index.js"
    },
    "./lib/index.css": "./lib/index.css"
  },
  "dependencies": {
    "react": "^18.3.1"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.26.0",
    "@linaria/core": "^6.2.0",
    "@linaria/react": "^6.2.1",
    "@types/node": "^22.9.0",
    "@types/react": "^18.3.12",
    "@wyw-in-js/babel-preset": "^0.5.4",
    "@wyw-in-js/esbuild": "^0.5.4",
    "@wyw-in-js/transform": "^0.5.4",
    "esbuild": "^0.24.0",
    "globby": "^14.0.2",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "yarn": "^1.22.22"
  }
}
